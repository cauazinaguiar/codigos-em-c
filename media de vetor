#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main(void) {
		setlocale(LC_ALL, "Portuguese");
		int vetor[]={10,6,30,4,40};
		int soma,i,tamanho;
		int media;
		media=0;
		tamanho=5;
		soma=0;
		
	for(i=0;i<tamanho;i++){
	
	soma+=vetor[i];
	
	}
	media=soma/tamanho;
	printf("\n a media dos vetores Ã©: %d", media);
		return 0;
	}
