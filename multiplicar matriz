#include <stdio.h>
#include <string.h>
#include <locale.h>
#include <stdlib.h>

int main() {
	setlocale(LC_ALL, "Portuguese");
    int matriz[3][3];
    int i,j,mult;
    
	for(i=0;i<3;i++){
		for(j=0;j<3;j++){
			printf("digite a matriz[%d][%d]", i, j);
			scanf("%d", &matriz[i][j]);
		}
	}
    puts("\nok");
    
    puts("agora escolha um numero para multiplicar a matriz");
    scanf("%d",&mult);
    
    puts("\na matriz Ã©:\n");
   for(i=0;i<3;i++){
		for(j=0;j<3;j++){
			printf("|%d| ",matriz[i][j]*mult);
		}
		printf("\n");
	}
    return 0;
}
